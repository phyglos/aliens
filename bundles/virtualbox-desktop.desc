#!/bin/bash

# Use different releases for x86 and x86_64
case $BANDIT_TARGET_ARCH in
    i?86)
        VBOX_VERSION="5.2.28"
        ;;
    x86_64)
        case $PHY_VBOX64 in
            "5.x") 
                VBOX_VERSION="5.2.28"
                ;;
            "new") 
                VBOX_VERSION="6.0.6"
                ;;
        esac
        ;;
esac

BUILD_CMD=(
"alien    do        virtualbox-$VBOX_VERSION"
)

